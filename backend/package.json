{
  "name": "reqflow-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "node scripts/write-build-info.cjs && tsc",
    "start": "node dist/index.js",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test": "npm run build && node --test tests/*.test.cjs",
    "test:coverage": "npm run build && node --test --experimental-test-coverage tests/*.test.cjs",
    "test:security": "npm run test"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.6.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "js-beautify": "^1.15.4",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.7.0",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.5",
    "@types/passport": "^1.0.12",
    "@types/passport-github2": "^1.1.0",
    "@types/passport-google-oauth20": "^2.0.11",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.0"
  }
}
